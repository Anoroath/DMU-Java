package opgave1;

public class Opgave1 {
    
    public static void main(String[] args) {
        factorial()
    }
    
    public int factorial(int n) {
        int result;
        if (n == 1) {
            result = 1;
        } else {
            result = result * n;
            factorial(n-1);
        }
        return result;
    }
}
